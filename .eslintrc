{
  "rules": {
    "indent": [0, 2],
    "quotes": [2, "double"],
    "linebreak-style": [1, "unix"],
    "semi": [1, "always"],
    "new-cap": [1],
    "eqeqeq": [1]
  },
  "env": {
    "es6": true,
    "browser": true,
    "amd": true
  },
  "globals": {
    "define": true,
    "require": true,
    "requirejs": true,

    "Str": true,
    "Locale": true,
    "Options": true
  }
}
